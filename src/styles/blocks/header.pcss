.header {
  padding-top: 52px;
  padding-bottom: 35px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.header__btns {
  font-size: 16px;
  font-weight: 500;
  white-space: nowrap;
  width: 65%;

  @include tablets {
    display: none;
    width: 0;
  }
}

.header__logo {
  font-size: 36px;
  font-weight: 800;
  // padding: 0 28px;
}

.header__links {

  @include tablets {
    margin-left: auto;

  }
}