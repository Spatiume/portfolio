.contacts {
  padding-bottom: 130px;
  background: url(./../images/bg/contacts.png) center center/cover no-repeat;
  position: relative;

}

.contacts__container {
  padding-top: 11.5%;
  padding-right: 11%;

  @include tablets {
    padding-right: 0%;
  }


  &::before {
    content: '';
    display: block;
    position: absolute;
    background: url(./../images/bg/leaves.png) center center/contain no-repeat;
    height: 1000px;
    width: 400px;
    top: -109px;
    right: -153px;
  }
}